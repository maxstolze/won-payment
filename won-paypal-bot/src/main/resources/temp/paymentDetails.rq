prefix s:     <http://schema.org/>
prefix won:   <http://purl.org/webofneeds/model#>
prefix pay:   <http://purl.org/webofneeds/payment#>
prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

Select ?amount ?currency ?receiver ?secret ?counterpart

where {
    ?main a pay:Payment ;
          pay:hasAmount ?amount ;
          pay:hasCurrency ?currency ;
          pay:hasReceiver ?receiver ;
          pay:hasSecret ?secret ;
          pay:hasNeedCounterpart ?counterpart .
}